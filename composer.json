{
    "name": "vinkla/facebook",
    "description": "A Facebook bridge for Laravel",
    "keywords": [
        "facebook",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "homepage": "https://github.com/vinkla"
        }
    ],
    "require": {
        "php": "^7.4",
        "facebook/graph-sdk": "^5.7",
        "graham-campbell/manager": "^4.4",
        "illuminate/contracts": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.0",
        "graham-campbell/testbench": "^5.4",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
            "providers": [
                "Vinkla\\Facebook\\FacebookServiceProvider"
            ],
            "aliases": {
                "Facebook": "Vinkla\\Facebook\\Facades\\Facebook"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Facebook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Facebook\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
